              <DbSource ConnectionRef="ConnectionString (MySettings)" DbObjectName="" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="FillPorIDParticipante" GenerateMethods="Both" GenerateShortCommands="true" GeneratorGetMethodName="GetCursosPorIDParticipante" GeneratorSourceName="FillPorIDParticipante" GetMethodModifier="Public" GetMethodName="GetCursosPorIDParticipante" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetCursosPorIDParticipante" UserSourceName="FillPorIDParticipante">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select pr.id_proveedor_af, p.id_participante, 
p.nombres, p.apellidos,
pr.nombre_proveedor, (select sc.nombre_sitio from sitio_capacitacion sc where sc.id_sitio_capacitacion = af.id_sitio_capacitacion)  as nombre_sitio, 
