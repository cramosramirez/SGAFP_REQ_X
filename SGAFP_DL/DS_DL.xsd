                    <CommandText>
 /* SELECT ej.id_ejercicio,
    ej.id_proveedor_af,
    ej.nombre_proveedor,
    SUM(ej.recursos)   AS recursos,
    SUM(ej.programado) AS programado,
    SUM(ej.ejecucion)  AS ejecucion,
    SUM(ej.finalizado) AS finalizado
    */
with 
proveedor as
(
  select v.ejercicio as id_ejercicio, v.id_proveedor_af, v.proveedor as nombre_proveedor, c.monto_asignado as recursos 
  from vsolic_ingresadas_htp v, contrato_provee_af c
  where v.id_proveedor_af = c.id_proveedor_af and v.ejercicio = c.id_ejercicio
  group by v.ejercicio, v.id_proveedor_af, v.proveedor, c.monto_asignado
),
finalizado as
(
  select id_proveedor_af, sum(costo_x_participante) as monto 
  from vsolic_ingresadas_htp 
  where fecha_fin_real &lt; sysdate
  group by id_proveedor_af
),
ejecucion as
(
  select id_proveedor_af, sum(costo_x_participante) as monto
  from vsolic_ingresadas_htp 
  where fecha_inicio_real &lt;= sysdate and fecha_fin_real &gt;= sysdate
  group by id_proveedor_af
),
programado as
(
  select id_proveedor_af, sum(costo_x_participante) as monto  
  from vsolic_ingresadas_htp 
  where fecha_inicio_real &gt; sysdate 
  group by id_proveedor_af
)

select proveedor.id_ejercicio, proveedor.id_proveedor_af, proveedor.nombre_proveedor,
      proveedor.recursos,
      nvl(programado.monto,0) as programado,
      nvl(ejecucion.monto,0) as ejecucion,
      nvl(finalizado.monto,0) as finalizado
from proveedor 
  left outer join finalizado on proveedor.id_proveedor_af = finalizado.id_proveedor_af 
  left outer join ejecucion on proveedor.id_proveedor_af = ejecucion.id_proveedor_af
  left outer join programado on proveedor.id_proveedor_af = programado.id_proveedor_af
order by nombre_proveedor</CommandText>
