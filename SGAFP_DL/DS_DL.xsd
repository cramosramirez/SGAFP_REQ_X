              <DbSource ConnectionRef="ConnectionString (MySettings)" DbObjectName="" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="FillPorIDParticipanteEjercicio" GenerateMethods="Both" GenerateShortCommands="true" GeneratorGetMethodName="GetCursosPorIDParticipanteEjercicio" GeneratorSourceName="FillPorIDParticipanteEjercicio" GetMethodModifier="Public" GetMethodName="GetCursosPorIDParticipanteEjercicio" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetCursosPorIDParticipanteEjercicio" UserSourceName="FillPorIDParticipanteEjercicio">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT AF.ID_PROVEEDOR_AF, P.ID_PARTICIPANTE, 
       V.NOMBRE_PROVEEDOR, AF.NOMBRE_ACCION_FORMATIVA, AF.ID_EJERCICIO, 
       S.NOMBRES, S.APELLIDOS, 
       (SELECT SC.NOMBRE_SITIO FROM SITIO_CAPACITACION SC WHERE SC.ID_SITIO_CAPACITACION = AF.ID_SITIO_CAPACITACION) AS NOMBRE_SITIO,
       NULL AS AREA_FORMACION, AF.COSTO_X_PARTICIPANTE, 
       (SELECT F.NOMBRE_PROGRAMA_FORMACION FROM ACCION_FORMATIVA_DETALLE AD, PROGRAMA_FORMACION F WHERE AD.ID_PROGRAMA_FORMACION = F.ID_PROGRAMA_FORMACION AND AD.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA) AS PROGRAMA,
       AF.CODIGO_GRUPO
FROM ACCION_FORMATIVA AF, PARTICIPANTE P, PARTICIPANTE_AF PAF, SOLICITUD_INSCRIPCION_AF S, PROVEEDOR_AF V
WHERE AF.ID_ACCION_FORMATIVA = S.ID_ACCION_FORMATIVA AND S.ID_PARTICIPANTE = P.ID_PARTICIPANTE
AND PAF.ID_SOLICITUD = S.ID_SOLICITUD
AND AF.ID_PROVEEDOR_AF = V.ID_PROVEEDOR_AF
AND AF.ID_EJERCICIO = :ID_EJERCICIO
AND PAF.ID_PARTICIPANTE = :ID_PARTICIPANTE
and paf.estado in ('I','V') 
AND (SELECT COUNT(1) FROM accion_formativa_detalle AD WHERE ad.id_accion_formativa = af.id_accion_formativa AND ad.id_programa_formacion = 1) &gt; 0</CommandText>
