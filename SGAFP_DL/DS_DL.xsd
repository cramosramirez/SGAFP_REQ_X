          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="PATIEjecucionTableAdapter" GeneratorDataComponentClassName="PATIEjecucionTableAdapter" Name="PATIEjecucion" UserDataComponentName="PATIEjecucionTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="ConnectionString (MySettings)" DbObjectName="" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>with cursos as(
    select af.id_accion_formativa, v.id_programa_formacion, pf.porc_asistencia_minima, 
    (select d.nombre from departamento d where d.codigo_departamento = v.codigo_departamento) as nombre_departamento, 
    (select m.nombre from municipio m where m.codigo_departamento = v.codigo_departamento and m.codigo_municipio = v.codigo_municipio) as nombre_municipio,
    p.nombre_proveedor as proveedor, trim(tdr.tdr) as tdr, trim(tdr.resolucion) as resolucion,
    af.nombre_accion_formativa, af.codigo_grupo, af.horario, af.fecha_inicio_real, af.fecha_fin_real, af.codigo_estado_af,
    (select sum(pana.solicitado_por_alcaldia) from pati_analisis_ejecucion pana where pana.id_accion_formativa = af.id_accion_formativa) as solicitados,     
    af.costo_x_participante, ff.costo_x_participante as costo_x_participante_fuente, 
    (case 
     when exists(select 1 from informe_final_af inf where inf.id_accion_formativa = af.id_accion_formativa) then
        (select einf.nombre_estado_informe from informe_final_af inf, estado_informe_af einf where inf.id_estado_informe = einf.id_estado_informe and inf.id_accion_formativa = af.id_accion_formativa)
     else
        (select e.nombre_estado_af from estado_accion_formativa e where e.codigo_estado_af = af.codigo_estado_af)
     end 
    ) as nombre_estado_af,    
    ff.nombre_fuente, v.no_convocatoria, af.notas,
    (select max(h.lastupdate) from informe_final_af_his h where h.id_accion_formativa = af.id_accion_formativa and h.id_estado_informe = 2) as fecha_recepcion
    from accion_formativa_detalle v, accion_formativa af, termino_referencia_af tdr, fuente_financiamiento ff, 
    proveedor_af p, programa_formacion pf
    where af.id_accion_formativa = v.id_accion_formativa    
    and af.id_accion_formativa = tdr.id_accion_formativa
    and ff.id_fuente = v.id_fuente
    and p.id_proveedor_af = af.id_proveedor_af
    and v.id_programa_formacion = pf.id_programa_formacion    
    and (ff.id_fuente = :id_tipo_solicitud or :id_tipo_solicitud = -1)
    and (p.id_proveedor_af = :id_proveedor_af or :id_proveedor_af = -1)
    and (v.no_convocatoria = :no_convocatoria or :no_convocatoria = -1)  
    and (af.codigo_estado_af = :codigo_estado_af or :codigo_estado_af is null)     
    and (trim(tdr.tdr) = :tdr or :tdr is null)
    and (trim(tdr.resolucion) = :resolucion or :resolucion is null)
),

participantes as 
(
    select cur.id_accion_formativa, decode(par.genero,'M',1,0) as hombre, decode(par.genero,'F',1,0) as mujer,
    case 
      when paf.porc_asistencia &gt;= p.porc_asistencia_minima then 1
      else 0
    end as es_activo,
    case
      when paf.porc_asistencia &gt;= 1.25 then 1
      else 0
    end as es_iniciado
    from participante_af paf, accion_formativa cur, participante par, accion_formativa_detalle d, programa_formacion p
    where paf.id_accion_formativa = cur.id_accion_formativa    
    and paf.id_participante = par.id_participante
    and cur.id_accion_formativa = d.id_accion_formativa
    and d.id_programa_formacion = p.id_programa_formacion
    and paf.estado in('I','V')    
    
),

documentos as(
  select inf.id_accion_formativa, fe.numero_documento, fe.numero_quedan, fe.fecha_quedan, fe.numero_cheque, fe.fecha_cheque,
  ( select ml.no_liquidacion from municipio_liquidacion ml where ml.id_liquidacion = md.id_liquidacion) as no_liquidacion,
  ( select ml.fecha_elaboracion from municipio_liquidacion ml where ml.id_liquidacion = md.id_liquidacion) as fecha_elaboracion
  from factura_af fe
  join factura_det fd on fe.id_factura_af = fd.id_factura_af
  join informe_final_af inf on inf.id_informe_final = fd.id_informe_final
  left outer join municipio_liquidacion_det md on inf.id_accion_formativa = md.id_accion_formativa 
  where fe.estado = 'P' and not fe.numero_quedan is null
)

select c.id_accion_formativa, c.nombre_departamento, c.nombre_municipio,
c.proveedor, c.tdr, c.resolucion, c.nombre_accion_formativa,
c.codigo_grupo, c.horario, c.fecha_inicio_real, c.fecha_fin_real, c.codigo_estado_af,
c.solicitados,
count(p.id_accion_formativa) as inscritos, sum(p.es_activo) as activos, 
(sum(p.es_activo) * c.costo_x_participante) as monto, 
c.costo_x_participante,
(sum(p.es_activo) * c.costo_x_participante_fuente) as monto_fuente,
c.nombre_estado_af,
sum(p.es_iniciado) as iniciados,
c.nombre_fuente, c.no_convocatoria, 
sum(p.hombre) as hombres, sum(p.mujer) as mujeres, 
c.notas, c.fecha_recepcion,
(select d.numero_documento from documentos d where d.id_accion_formativa = c.id_accion_formativa) as numero_documento,
(select d.numero_quedan from documentos d where d.id_accion_formativa = c.id_accion_formativa) as numero_quedan,
(select d.fecha_quedan from documentos d where d.id_accion_formativa = c.id_accion_formativa) as fecha_quedan,
(select d.numero_cheque from documentos d where d.id_accion_formativa = c.id_accion_formativa) as numero_cheque,
(select d.fecha_cheque from documentos d where d.id_accion_formativa = c.id_accion_formativa) as fecha_cheque,
(select d.no_liquidacion from documentos d where d.id_accion_formativa = c.id_accion_formativa) as no_liquidacion,
(select d.fecha_elaboracion from documentos d where d.id_accion_formativa = c.id_accion_formativa) as fecha_elaboracion,
(select sum(p.hombre) from participantes p where p.id_accion_formativa = c.id_accion_formativa and p.es_activo = 1) as hombres_capacitados,
(select sum(p.mujer) from participantes p where p.id_accion_formativa = c.id_accion_formativa and p.es_activo = 1) as mujeres_capacitadas,
(select sum(p.hombre) from participantes p where p.id_accion_formativa = c.id_accion_formativa and p.es_activo = 1) * c.costo_x_participante as hombres_capacitados_monto,
(select sum(p.mujer) from participantes p where p.id_accion_formativa = c.id_accion_formativa and p.es_activo = 1) * c.costo_x_participante as mujeres_capacitadas_monto
from cursos c, participantes p
where c.id_accion_formativa = p.id_accion_formativa
group by 
c.id_accion_formativa, c.nombre_departamento, c.nombre_municipio,
c.proveedor, c.tdr, c.resolucion, c.nombre_accion_formativa,
c.codigo_grupo, c.horario, c.fecha_inicio_real, c.fecha_fin_real, c.codigo_estado_af,
c.solicitados, 
c.costo_x_participante,
c.nombre_estado_af,
c.nombre_fuente, c.no_convocatoria, 
c.notas, c.fecha_recepcion, c.costo_x_participante, c.costo_x_participante_fuente

