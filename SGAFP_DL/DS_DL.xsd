          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="CursosPATI_Etapas_DuracionTableAdapter" GeneratorDataComponentClassName="CursosPATI_Etapas_DuracionTableAdapter" Name="CursosPATI_Etapas_Duracion" UserDataComponentName="CursosPATI_Etapas_DuracionTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="ConnectionString (MySettings)" DbObjectName="" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>with 
  af_pati as
  (
  select tdr.tdr, tdr.resolucion, p.nombre_proveedor, af.id_accion_formativa, af.nombre_accion_formativa, af.fecha_inicio_real, af.fecha_fin_real, af.codigo_estado_af,
  af.codigo_grupo, (select de.nombre from departamento de where de.codigo_departamento = d.codigo_departamento) as departamento,
  (select mu.nombre from municipio mu where mu.codigo_departamento = d.codigo_departamento and mu.codigo_municipio = d.codigo_municipio) as municipio
  from accion_formativa af
  join termino_referencia_af tdr on af.id_accion_formativa = tdr.id_accion_formativa  
  join proveedor_af p on p.id_proveedor_af = af.id_proveedor_af 
  join accion_formativa_detalle d on d.id_accion_formativa = af.id_accion_formativa
  where 
    exists(select 1 from accion_formativa_detalle v 
           where v.id_accion_formativa = af.id_accion_formativa and v.id_fuente = :id_fuente and v.no_convocatoria = :no_convocatoria)        
    and af.codigo_estado_af not in('01','02','03','09')
  ),
  af_recibidas as
  (
    select id_accion_formativa, lastupdate
    from informe_final_af af
    where id_estado_informe = 2 and not exists(select 1 from informe_final_af_his ah where ah.id_informe_final = af.id_informe_final and ah.id_estado_informe = 2)
    union all
    select af.id_accion_formativa, max(ah.lastupdate) as lastupdate
    from informe_final_af af, informe_final_af_his ah
    where af.id_informe_final = ah.id_informe_final
    and ah.id_estado_informe = 2
    group by af.id_accion_formativa
  ),
  af_aceptadasGFI as
  (
    select id_accion_formativa, lastupdate
    from informe_final_af af
    where id_estado_informe = 9 and not exists(select 1 from informe_final_af_his ah where ah.id_informe_final = af.id_informe_final and ah.id_estado_informe = 9)
    union all
    select af.id_accion_formativa, max(ah.lastupdate) as lastupdate
    from informe_final_af af, informe_final_af_his ah
    where af.id_informe_final = ah.id_informe_final
    and ah.id_estado_informe = 9
    group by af.id_accion_formativa
  ),
  af_facturaSolic as
  (
    select id_accion_formativa, lastupdate
    from informe_final_af af
    where id_estado_informe = 10 and not exists(select 1 from informe_final_af_his ah where ah.id_informe_final = af.id_informe_final and ah.id_estado_informe = 10)
    union all
    select af.id_accion_formativa, min(ah.lastupdate) as lastupdate
    from informe_final_af af, informe_final_af_his ah
    where af.id_informe_final = ah.id_informe_final
    and ah.id_estado_informe = 10
    group by af.id_accion_formativa
  ),
  af_facturaAprobada as
  (
    select id_accion_formativa, lastupdate
    from informe_final_af af
    where id_estado_informe = 11 and not exists(select 1 from informe_final_af_his ah where ah.id_informe_final = af.id_informe_final and ah.id_estado_informe = 11)
    union all
    select af.id_accion_formativa, max(ah.lastupdate) as lastupdate
    from informe_final_af af, informe_final_af_his ah
    where af.id_informe_final = ah.id_informe_final
    and ah.id_estado_informe = 11
    group by af.id_accion_formativa
  ),
  af_conQuedan as
  (
    select af.id_accion_formativa, f1.fecha_quedan as lastupdate, f1.numero_quedan
    from informe_final_af af, factura_af f1, factura_det f2
    where af.id_informe_final = f2.id_informe_final
    and f1.id_factura_af = f2.id_factura_af   
    and not f1.numero_quedan is null
  ),
  af_conCheque as
  (
    select af.id_accion_formativa, f1.fecha_cheque as lastupdate
    from informe_final_af af, factura_af f1, factura_det f2
    where af.id_informe_final = f2.id_informe_final
    and f1.id_factura_af = f2.id_factura_af   
    and not f1.numero_cheque is null
  ),
  af_conLiquidacion as
  (
    select md.id_accion_formativa, ml.fecha_elaboracion as lastupdate
    from municipio_liquidacion ml, municipio_liquidacion_det md
    where ml.id_liquidacion = md.id_liquidacion
  )
select t6.numero_quedan, t1.resolucion, t1.nombre_proveedor, t1.id_accion_formativa, t1.nombre_accion_formativa,t1.codigo_grupo,t1.departamento,
t1.municipio,
to_char(t1.fecha_inicio_real,'dd/mm/yyyy') as inicio_curso,
to_char(t1.fecha_fin_real,'dd/mm/yyyy') as finalizo_curso, to_char(t2.lastupdate,'dd/mm/yyyy') as recibido_insaforp, 
round(t2.lastupdate - t1.fecha_fin_real,0) as dias_trans_finalizo_recib,
to_char(t3.lastupdate,'dd/mm/yyyy') as aceptado_gfi_entregado_uaci, 
trunc(t3.lastupdate - t2.lastupdate,0) as dias_trans_recib_aceptado, 
round(t3.lastupdate - t1.fecha_fin_real,0) as dias_acum_finalizo_aceptado,
to_char(t4.lastupdate,'dd/mm/yyyy') as factura_solicitada, 
trunc(t4.lastupdate - t3.lastupdate,0) as dias_trans_aceptado_solfact, 
round(t4.lastupdate - t1.fecha_fin_real,0) as dias_acum_finalizo_solfact,
to_char(t5.lastupdate,'dd/mm/yyyy') as factura_aprobada, 
trunc(t5.lastupdate - t4.lastupdate,0) as dias_trans_solfact_aprofact, 
round(t5.lastupdate - t1.fecha_fin_real,0) as dias_acum_finalizo_aprofact,
to_char(t6.lastupdate,'dd/mm/yyyy') as quedan_emitido, 
trunc(t6.lastupdate - t5.lastupdate,0) as dias_trans_aprofact_quedan, 
round(t6.lastupdate - t1.fecha_fin_real,0) as dias_acum_finalizo_quedan,
to_char(t7.lastupdate,'dd/mm/yyyy') as cheque_emitido, 
trunc(t7.lastupdate - t6.lastupdate,0) as dias_trans_quedan_cheque,
round(t7.lastupdate - t1.fecha_fin_real,0) as dias_acum_finalizo_cheque,
to_char(t8.lastupdate,'dd/mm/yyyy') as fecha_liquidacion, 
case 
  when :id_fuente = 1 then trunc(t7.lastupdate - t8.lastupdate,0)  
  else trunc(t8.lastupdate - t7.lastupdate,0) 
  end as dias_trans_cheque_liqui,
round(t8.lastupdate - t1.fecha_fin_real,0) as dias_acum_finalizo_liqui
from af_pati t1 
left join af_recibidas t2 on t1.id_accion_formativa = t2.id_accion_formativa
left join af_aceptadasgfi t3 on t1.id_accion_formativa = t3.id_accion_formativa
left join af_facturasolic t4 on t1.id_accion_formativa = t4.id_accion_formativa
left join af_facturaaprobada t5 on t1.id_accion_formativa = t5.id_accion_formativa
left join af_conquedan t6 on t1.id_accion_formativa = t6.id_accion_formativa
LEFT JOIN AF_CONCHEQUE T7 ON T1.ID_ACCION_FORMATIVA = T7.ID_ACCION_FORMATIVA
left join af_conLiquidacion t8 on t1.id_accion_formativa = t8.id_accion_formativa</CommandText>
