                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
  CASE 
    WHEN :NO_LIQUIDACION = 1 THEN ROWNUM 
    ELSE (SELECT MAX(ML.ID_LIQUIDACION) FROM MUNICIPIO_LIQUIDACION ML 
          WHERE ML.ID_FUENTE = :ID_FUENTE 
          AND ML.NO_CONVOCATORIA = :NO_CONVOCATORIA 
          AND ML.CODIGO_DEPARTAMENTO = MC.CODIGO_DEPARTAMENTO
          AND ML.CODIGO_MUNICIPIO = MC.CODIGO_MUNICIPIO
          AND ML.NO_LIQUIDACION = :NO_LIQUIDACION - 1
          )
  END AS ID_LIQUIDACION,
  :NO_LIQUIDACION AS NO_LIQUIDACION,
  (MC.APORTE_FISDL*0.5) AS MONTO_LIQUIDADO,
  MC.CODIGO_PROYECTO,
  MC.NOMBRE_PROYECTO,
  MUNICIPIO.NOMBRE    AS NOMBRE_MUNICIPIO,
  DEPARTAMENTO.NOMBRE AS NOMBRE_DEPARTAMENTO,
  CASE 
    WHEN FM.CODIGO_FINANCIAMIENTO IS NULL THEN FF.CODIGO_FINANCIAMIENTO
    ELSE FM.CODIGO_FINANCIAMIENTO
  END AS CODIGO_FINANCIAMIENTO,  
  MC.ASESOR_MUNICIPAL,
  CASE 
    WHEN FM.NOMBRE_BANCO_DESEMBOLSO IS NULL THEN FF.NOMBRE_BANCO_DESEMBOLSO
    ELSE FM.NOMBRE_BANCO_DESEMBOLSO
  END AS NOMBRE_BANCO_DESEMBOLSO,
  CASE 
    WHEN FM.NO_CTA_DESEMBOLSO IS NULL THEN FF.NO_CTA_DESEMBOLSO
    ELSE FM.NO_CTA_DESEMBOLSO
  END AS NO_CTA_DESEMBOLSO,
  CASE 
    WHEN FM.NOMBRE_CTA_DESEMBOLSO IS NULL THEN FF.NOMBRE_CTA_DESEMBOLSO
    ELSE FM.NOMBRE_CTA_DESEMBOLSO
  END AS NOMBRE_CTA_DESEMBOLSO,  
  FF.ID_FUENTE
FROM FUENTE_FINANCIAMIENTO FF
JOIN MUNICIPIO_CONVOCATORIA MC
ON FF.ID_FUENTE = MC.ID_FUENTE
JOIN MUNICIPIO
ON MUNICIPIO.CODIGO_DEPARTAMENTO = MC.CODIGO_DEPARTAMENTO
