AND M.CODIGO_MUNICIPIO = F.CODIGO_MUNICIPIO
AND P.CODIGO_DEPARTAMENTO = M.CODIGO_DEPARTAMENTO
AND P.CODIGO_MUNICIPIO = M.CODIGO_MUNICIPIO
AND P.ID_PLAN = G.ID_PLAN
AND GPS.ID_GRUPO_AF = G.ID_GRUPO_AF
AND S.ID_SOLICITUD = GPS.ID_SOLICITUD
AND F.ID_FUENTE = :ID_FUENTE
