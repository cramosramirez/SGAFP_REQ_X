            <MainSource>
              <DbSource ConnectionRef="ConnectionString (MySettings)" DbObjectName="" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>with 
acciones as
(
  select p.nombre_proveedor as proveedor, area.id_area_formacion as id_area_formativa, area.area_formacion as area_formativa,
  af.id_accion_formativa as id_curso, af.nombre_accion_formativa as curso
  from accion_formativa af, curso_tema ct, area_formacion area, proveedor_af p 
  where af.id_tema_curso = ct.id_tema_curso 
  and ct.id_area_formacion = area.id_area_formacion 
  and af.id_proveedor_af = p.id_proveedor_af
  and af.id_proveedor_af = :ID_PROVEEDOR_AF and af.id_ejercicio = :ID_EJERCICIO  
  and not exists(select 1 from termino_referencia_af tdr where tdr.id_accion_formativa = af.id_accion_formativa)
  group by p.nombre_proveedor, area.id_area_formacion, area.area_formacion, af.id_accion_formativa, af.nombre_accion_formativa
),
ejecutado as
(
  select id_curso,
  sum(costo_x_participante) as monto, count(id_solicitud) as participantes
  from vsolic_ingresadas_htp 
  where id_proveedor_af = :ID_PROVEEDOR_AF and ejercicio = :ID_EJERCICIO and fecha_fin_real &lt; to_char(sysdate,'dd/MON/yyyy')
  group by id_curso
),
ejecucion as
(
  select id_curso,
  sum(costo_x_participante) as monto, count(id_solicitud) as participantes
  from vsolic_ingresadas_htp 
  where id_proveedor_af = :ID_PROVEEDOR_AF and ejercicio = :ID_EJERCICIO and fecha_inicio_real &lt;= to_char(sysdate,'dd/MON/yyyy') and fecha_fin_real &gt;= to_char(sysdate,'dd/MON/yyyy')
  group by id_curso
),
programado_no_inscritos as
(
  select af.id_accion_formativa as id_curso,
  sum(af.costo_x_participante * af.numero_participantes) monto, sum(af.numero_participantes) as participantes
  from accion_formativa af, curso_tema ct, area_formacion area
  where af.id_proveedor_af = :ID_PROVEEDOR_AF and af.id_ejercicio = :ID_EJERCICIO and af.fecha_inicio_real &gt; to_char(sysdate,'dd/MON/yyyy') 
  and af.id_tema_curso = ct.id_tema_curso and ct.id_area_formacion = area.id_area_formacion and af.participantes_inscritos = 0
  and not exists(select 1 from termino_referencia_af tdr where tdr.id_accion_formativa = af.id_accion_formativa)
  group by af.id_accion_formativa
),
programado as
(
  select id_curso, sum(monto) as monto, sum(participantes) as participantes
  from
  (
    select id_curso, 
    sum(costo_x_participante) as monto, count(id_solicitud) as participantes
    from vsolic_ingresadas_htp 
    where id_proveedor_af = :ID_PROVEEDOR_AF and ejercicio = :ID_EJERCICIO and fecha_inicio_real &gt; to_char(sysdate,'dd/MON/yyyy') 
    group by id_curso  
    
    union all
    
    select id_curso, monto, participantes
    from programado_no_inscritos 
  )
  group by id_curso
)

select proveedor, id_area_formativa, area_formativa, curso,
       sum(ejecutado_monto) as ejecutado_monto,       
       sum(ejecutado_participantes) as ejecutado_participantes,
       count(ejecutado_num_cursos) as ejecutado_num_cursos, 
       sum(ejecucion_monto) as ejecucion_monto,       
       sum(ejecucion_participantes) as ejecucion_participantes,
       count(ejecucion_num_cursos) as ejecucion_num_cursos, 
       sum(programado_monto) as programado_monto,       
       sum(programado_participantes) as programado_participantes,
       count(programado_num_cursos) as programado_num_cursos,       
       recurso_area_formativa
from
(
select      
        acciones.proveedor, acciones.id_area_formativa, acciones.area_formativa, acciones.curso,        
        nvl(ejecutado.monto,0) as ejecutado_monto,      
        nvl(ejecutado.participantes,0) as ejecutado_participantes,
        ejecutado.id_curso as ejecutado_num_cursos,
        nvl(ejecucion.monto,0) as ejecucion_monto,      
        nvl(ejecucion.participantes,0) as ejecucion_participantes,
        ejecucion.id_curso as ejecucion_num_cursos,
        nvl(programado.monto,0) as programado_monto,      
        nvl(programado.participantes,0) as programado_participantes,   
        programado.id_curso as programado_num_cursos,
        sum(nvl(ejecutado.monto,0) + nvl(ejecucion.monto,0) + nvl(programado.monto,0)) over(partition by acciones.id_area_formativa) as recurso_area_formativa
  from acciones 
    left outer join ejecutado on acciones.id_curso = ejecutado.id_curso 
    left outer join ejecucion on acciones.id_curso = ejecucion.id_curso
    left outer join programado on acciones.id_curso = programado.id_curso
)
group by proveedor, id_area_formativa, area_formativa, curso, recurso_area_formativa</CommandText>
