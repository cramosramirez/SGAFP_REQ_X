                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT INF.ID_INFORME_FINAL, 
 CASE 
  WHEN EXISTS(SELECT 1 FROM TERMINO_REFERENCIA_AF TDR,  SOLICITUD_CAPACITACION SC, PROYECTO_FORMACION Y WHERE TDR.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA AND SC.ID_SOLICITUD = TDR.ID_SOLICITUD AND SC.ID_PROYECTO_FORMACION = Y.ID_PROYECTO_FORMACION) THEN
    (SELECT Y.NOMBRE_PROYECTO_FORMACION FROM TERMINO_REFERENCIA_AF TDR, SOLICITUD_CAPACITACION SC, PROYECTO_FORMACION Y WHERE TDR.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA AND SC.ID_SOLICITUD = TDR.ID_SOLICITUD AND SC.ID_PROYECTO_FORMACION = Y.ID_PROYECTO_FORMACION)
  ELSE
    (SELECT F.NOMBRE_PROGRAMA_FORMACION FROM PROGRAMA_FORMACION F WHERE F.ID_PROGRAMA_FORMACION = AFD.ID_PROGRAMA_FORMACION)
END AS NOMBRE_PROYECTO_FORMACION, 
  (SELECT R.NOMBRE_REFERENTE 
  FROM SOLICITUD_CAPACITACION SC, REFERENTE R, TERMINO_REFERENCIA_AF TDR
  WHERE SC.ID_REFERENTE = R.ID_REFERENTE AND SC.ID_SOLICITUD = TDR.ID_SOLICITUD AND TDR.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA) AS NOMBRE_REFERENTE, 
P.NOMBRE_PROVEEDOR, MO.MODALIDAD_FORMACION, 
CASE 
WHEN (SELECT 1 FROM TERMINO_REFERENCIA_AF TDR WHERE TDR.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA) &gt; 0 THEN (SELECT TDR.RESOLUCION FROM TERMINO_REFERENCIA_AF TDR WHERE TDR.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA)
ELSE (SELECT C.NUM_CONTRATO FROM CONTRATO_PROVEEDOR_AF C WHERE AFD.ID_CONTRATO = C.ID_CONTRATO)
END AS RESOLUCION,
AF.NOMBRE_ACCION_FORMATIVA, AF.CODIGO_GRUPO, AF.HORARIO, AF.DURACION_HORAS, INF.HORAS_DESARROLLADAS,
