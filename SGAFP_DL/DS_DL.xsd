0 AS ID_PERIODO,
 '' AS PERIODO,  SYSDATE AS FECHA_INICIAL, SYSDATE AS FECHA_FINAL,
  PARTICIPANTE.ID_PARTICIPANTE 
         FROM PARTICIPANTE_AF, ASISTENCIA_AF, ACCION_FORMATIVA, PARTICIPANTE, PROVEEDOR_AF
WHERE PARTICIPANTE_AF.ID_ACCION_FORMATIVA = ACCION_FORMATIVA.ID_ACCION_FORMATIVA 
