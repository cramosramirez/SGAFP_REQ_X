    (SELECT F.NOMBRE_PROGRAMA_FORMACION FROM PROGRAMA_FORMACION F WHERE F.ID_PROGRAMA_FORMACION = AFD.ID_PROGRAMA_FORMACION)
END AS NOMBRE_PROYECTO_FORMACION, 
P.NOMBRE_PROVEEDOR, 
AF.NOMBRE_ACCION_FORMATIVA, AF.NOTAS AS INSTRUCTOR, AFD.LUGAR_EJECUCION,
(SELECT D.NOMBRE FROM DEPARTAMENTO D WHERE D.CODIGO_DEPARTAMENTO = AFD.CODIGO_DEPARTAMENTO) AS DEPARTAMENTO,
(SELECT M.NOMBRE FROM MUNICIPIO M WHERE M.CODIGO_DEPARTAMENTO = AFD.CODIGO_DEPARTAMENTO AND M.CODIGO_MUNICIPIO = AFD.CODIGO_MUNICIPIO) AS MUNICIPIO,
MO.MODALIDAD_FORMACION, INF.HORAS_DESARROLLADAS, 
(SELECT SUM(F.HORAS_DESARROLLADAS) FROM INFORME_FINAL_AF F WHERE F.ID_ACCION_FORMATIVA = INF.ID_ACCION_FORMATIVA AND F.FECHA_INICIAL &lt;= INF.FECHA_INICIAL) AS HORAS_ACUMULADAS,
INF.HORARIO, AF.CODIGO_GRUPO, AF.DURACION_HORAS, INF.FECHA_INICIAL, INF.FECHA_FINAL,
CONCAT(CONCAT(SOL.APELLIDOS, ', '), SOL.NOMBRES) AS NOMBRES_APELLIDOS, DET.NOTA_TEORICA, 
DET.NOTA_PRACTICA, DET.NOTA_OBTENIDA AS NOTA_FINAL, DET.PORC_ASISTENCIA, 
DET.NOTAS, DET.ID_PARTICIPANTE,
SOL.DUI, SOL.NIT
FROM INFORME_FINAL_AF INF, ACCION_FORMATIVA AF, 
ACCION_FORMATIVA_DETALLE AFD, TERMINO_REFERENCIA_AF TDR,
PROVEEDOR_AF P, MODALIDAD_DE_FORMACION MO, INFORME_FINAL_DET DET,
SOLICITUD_INSCRIPCION_AF SOL
WHERE INF.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA  
AND AF.ID_ACCION_FORMATIVA = AFD.ID_ACCION_FORMATIVA
AND AF.ID_ACCION_FORMATIVA = TDR.ID_ACCION_FORMATIVA
AND AF.ID_PROVEEDOR_AF = P.ID_PROVEEDOR_AF
AND AFD.ID_MODALIDAD_FORMACION = MO.ID_MODALIDAD_FORMACION
AND INF.ID_INFORME_FINAL = DET.ID_INFORME_FINAL
AND DET.ID_PARTICIPANTE = SOL.ID_PARTICIPANTE  
AND INF.ID_ACCION_FORMATIVA = SOL.ID_ACCION_FORMATIVA 
AND INF.ID_INFORME_FINAL = :ID_INFORME_FINAL </CommandText>
