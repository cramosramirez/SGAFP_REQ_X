                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT ID_ACCION_FORMATIVA, NOMBRE_ACCION_FORMATIVA, FECHA_INICIO_REAL, FECHA_FIN_REAL,
  PARTICIPANTES_FINALES, 
  CASE 
    WHEN CONVENIO IS NULL THEN (SELECT MAX(DF.NUME_FAMI) FROM FICHA_PATI FP, DETA_FAMI DF
      WHERE FP.ID_PARTICIPANTE = T.ID_PARTICIPANTE AND FP.NUME_FICH = DF.NUME_FICH_PATI)
    ELSE CONVENIO
    END AS CONVENIO, CODIGO_DEPARTAMENTO, CODIGO_MUNICIPIO, DEPARTAMENTO, MUNICIPIO, ID_ESTADO, ID_PARTICIPANTE 
FROM
(
  SELECT AF.ID_ACCION_FORMATIVA, AF.NOMBRE_ACCION_FORMATIVA, AF.FECHA_INICIO_REAL, AF.FECHA_FIN_REAL,
  INF.PARTICIPANTES_FINALES, 
  ( SELECT MAX(DF.NUME_FAMI) FROM FICHA_PATI FP, DETA_FAMI DF
    WHERE FP.ID_FUENTE = DF.ID_FUENTE 
    AND FP.NO_CONVOCATORIA = DF.NO_CONVOCATORIA
    AND FP.ID_PARTICIPANTE = IND.ID_PARTICIPANTE
    AND FP.ID_FUENTE = AD.ID_FUENTE
    AND FP.NO_CONVOCATORIA = AD.NO_CONVOCATORIA
    AND FP.NUME_FICH = DF.NUME_FICH_PATI) AS CONVENIO, AD.CODIGO_DEPARTAMENTO, AD.CODIGO_MUNICIPIO,
  (SELECT D.NOMBRE FROM DEPARTAMENTO D WHERE D.CODIGO_DEPARTAMENTO = AD.CODIGO_DEPARTAMENTO) AS DEPARTAMENTO,
  (SELECT M.NOMBRE FROM MUNICIPIO M WHERE M.CODIGO_DEPARTAMENTO = AD.CODIGO_DEPARTAMENTO AND M.CODIGO_MUNICIPIO = AD.CODIGO_MUNICIPIO) AS MUNICIPIO,
  IND.ID_PARTICIPANTE,
  CASE 
  WHEN IND.PORC_ASISTENCIA &gt;= (SELECT PF.PORC_ASISTENCIA_MINIMA FROM PROGRAMA_FORMACION PF WHERE PF.ID_PROGRAMA_FORMACION = AD.ID_PROGRAMA_FORMACION) THEN 1009 
  ELSE 1010
  END AS ID_ESTADO
  FROM ACCION_FORMATIVA AF, ACCION_FORMATIVA_DETALLE AD, INFORME_FINAL_AF INF, INFORME_FINAL_DET IND  
  WHERE AF.ID_ACCION_FORMATIVA = AD.ID_ACCION_FORMATIVA
  AND AF.ID_ACCION_FORMATIVA = INF.ID_ACCION_FORMATIVA
  AND INF.ID_INFORME_FINAL = IND.ID_INFORME_FINAL
  AND AD.ID_FUENTE = :ID_FUENTE
  AND AD.NO_CONVOCATORIA = :NO_CONVOCATORIA  
  AND EXISTS(SELECT 1 FROM PARTICIPANTE_AF PAF 
