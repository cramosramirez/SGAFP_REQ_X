                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>WITH SEMANAS AS(
  SELECT FECHA_FIN_SEMANA, MIN(FECHA) AS INICIO, MAX(FECHA) AS FIN
  FROM
  (
    SELECT FECHA, NEXT_DAY(FECHA,'DOM') AS FECHA_FIN_SEMANA
    FROM ASISTENCIA_AF 
    WHERE ID_ACCION_FORMATIVA = :ID_ACCION_FORMATIVA 
    AND FECHA &gt; = :FECHA_INICIAL AND FECHA &lt;= :FECHA_FINAL 
  )             
  GROUP BY FECHA_FIN_SEMANA
),

PARTICIPANTES AS(
  SELECT ROWNUM AS CORRELATIVO, PAF.ID_PARTICIPANTE, PAR.APELLIDOS || ' ' || PAR.NOMBRES AS NOMBRE_COMPLETO, AF.ID_ACCION_FORMATIVA
  FROM ACCION_FORMATIVA AF, PARTICIPANTE_AF PAF, PARTICIPANTE PAR
  WHERE AF.ID_ACCION_FORMATIVA = :ID_ACCION_FORMATIVA
  AND AF.ID_ACCION_FORMATIVA = PAF.ID_ACCION_FORMATIVA  
  AND PAF.ESTADO IN('I','V')
  AND PAF.ID_PARTICIPANTE = PAR.ID_PARTICIPANTE  
  ORDER BY PAR.APELLIDOS, PAR.NOMBRES
),

ASISTENCIA AS(
  SELECT    
  NEXT_DAY(FECHA,'DOM') AS FECHA_FIN_SEMANA,
  PARTICIPANTES.NOMBRE_COMPLETO, 
  ASISTENCIA_AF.FECHA, 
  NVL((SELECT DISTINCT HORAS_ASISTENCIA FROM ASISTENCIA_AF_DET, ASISTENCIA_AF AAF 
           WHERE AAF.ID_ASISTENCIA_AF = ASISTENCIA_AF_DET.ID_ASISTENCIA_AF
                AND AAF.ID_ACCION_FORMATIVA = PARTICIPANTES.ID_ACCION_FORMATIVA 
                AND ASISTENCIA_AF_DET.ID_PARTICIPANTE = PARTICIPANTES.ID_PARTICIPANTE 
                AND  AAF.ID_ASISTENCIA_AF = ASISTENCIA_AF.ID_ASISTENCIA_AF), ASISTENCIA_AF.CANTIDAD_HORAS) AS HORAS_ASISTIDAS,
  PARTICIPANTES.ID_PARTICIPANTE, PARTICIPANTES.CORRELATIVO  
  FROM PARTICIPANTES, ASISTENCIA_AF, ACCION_FORMATIVA
  WHERE PARTICIPANTES.ID_ACCION_FORMATIVA = ACCION_FORMATIVA.ID_ACCION_FORMATIVA 
       AND ASISTENCIA_AF.ID_ACCION_FORMATIVA = ACCION_FORMATIVA.ID_ACCION_FORMATIVA             
       AND (ACCION_FORMATIVA.ID_ACCION_FORMATIVA = :ID_ACCION_FORMATIVA)        
  AND ASISTENCIA_AF.FECHA &gt;= :FECHA_INICIAL AND ASISTENCIA_AF.FECHA &lt;= :FECHA_FINAL
)

SELECT CORRELATIVO, ID_PARTICIPANTE, NOMBRE_COMPLETO, PERIODO, FECHA_FIN_SEMANA, SUM(HORAS_ASISTIDAS) AS HORAS_ASISTIDAS
FROM
(
  SELECT A.CORRELATIVO, A.ID_PARTICIPANTE, A.NOMBRE_COMPLETO, A.HORAS_ASISTIDAS, A.FECHA_FIN_SEMANA,
    CASE 
    WHEN EXTRACT(DAY FROM INICIO) = EXTRACT(DAY FROM FIN) THEN TO_CHAR(EXTRACT(DAY FROM INICIO)) || ' ' || TO_CHAR(FIN,'MONTH')
    ELSE TO_CHAR(EXTRACT(DAY FROM INICIO)) || ' AL ' || TO_CHAR(EXTRACT(DAY FROM FIN)) || ' ' || TO_CHAR(FIN,'MONTH')
    END AS PERIODO
  FROM ASISTENCIA A, SEMANAS S
  WHERE A.FECHA_FIN_SEMANA = S.FECHA_FIN_SEMANA
) GROUP BY CORRELATIVO, ID_PARTICIPANTE, NOMBRE_COMPLETO, PERIODO, FECHA_FIN_SEMANA

