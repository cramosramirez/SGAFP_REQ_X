     AND (ACCION_FORMATIVA.ID_ACCION_FORMATIVA = :ID_ACCION_FORMATIVA) 
     AND (PARTICIPANTE_AF.ESTADO IN ('I', 'V'))  
ORDER  BY ASISTENCIA_AF.FECHA, PARTICIPANTE.APELLIDOS, PARTICIPANTE.NOMBRES
</CommandText>
