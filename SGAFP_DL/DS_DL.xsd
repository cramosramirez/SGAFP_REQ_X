      af.id_accion_formativa as id_curso, 
      (select area.area_formacion from area_formacion area, curso_tema ct where area.id_area_formacion = ct.id_area_formacion and ct.id_tema_curso = af.id_tema_curso) as area_formativa, 
      af.nombre_accion_formativa as curso, af.fecha_inicio_real, af.fecha_fin_real, af.horario,
      e.nombre_estado_af, 
      d.lugar_ejecucion as direccion, 
      (select sc.encargado from sitio_capacitacion sc where sc.id_sitio_capacitacion = af.id_sitio_capacitacion) as encargado, 
      (select sc.email from sitio_capacitacion sc where sc.id_sitio_capacitacion = af.id_sitio_capacitacion) as email, 
      (select sc.telefono from sitio_capacitacion sc where sc.id_sitio_capacitacion = af.id_sitio_capacitacion) as telefono,
      af.notas, af.codigo_grupo, af.duracion_horas, 
      (select depto.nombre from departamento depto where depto.codigo_departamento = d.codigo_departamento) as departamento, 
      (select muni.nombre from municipio muni where muni.codigo_departamento = d.codigo_departamento and muni.codigo_municipio = d.codigo_municipio) as municipio,      
      case 
        when af.fecha_inicio_real &gt; to_char(sysdate,'dd/MON/yyyy') then 1
        when af.fecha_inicio_real &lt;= to_char(sysdate,'dd/MON/yyyy') and af.fecha_fin_real &gt;= to_char(sysdate,'dd/MON/yyyy') then 2
        else 3
      end clasificacion,
      (select c.num_contrato from contrato_proveedor_af c where c.id_contrato = d.id_contrato) as num_contrato,      
      (select sub.nombre_subarea from sub_area_formacion sub, curso_tema ct  where sub.id_subarea_formacion = ct.id_subarea_formacion and ct.id_tema_curso = af.id_tema_curso) as nombre_sub_area,
      nvl((select csub.monto_adjudicado from contrato_sub_area_formacion csub, sub_area_formacion sa, curso_tema ct 
            where csub.id_subarea_formacion = sa.id_subarea_formacion 
            and sa.id_subarea_formacion = ct.id_subarea_formacion 
            and ct.id_tema_curso = af.id_tema_curso 
            and csub.id_contrato = d.id_contrato),0) as adjudicado_subarea,
      af.costo_x_participante,       
      (select to_char(c.num_licitacion) from contrato_proveedor_af c where c.id_contrato = d.id_contrato) as num_licitacion, 
      (select c.id_ejercicio from contrato_proveedor_af c where c.id_contrato = d.id_contrato) as ejercicio_licitacion,
      (select max(u.nombre) from tecnicos_sitios_asig t, usuario u 
      where t.id_sitio_capacitacion = af.id_sitio_capacitacion and t.usuario = u.usuario) as tecnico_asignado, af.lastupdate as actualizacion,
      (select min(e.orden) from informe_final_af i, estado_informe_af e where i.id_accion_formativa = af.id_accion_formativa and i.id_estado_informe = e.id_estado_informe) as orden,
      (select min(h.lastupdate) from accion_formativa_hist h where h.id_accion_formativa = af.id_accion_formativa and h.codigo_estado_af = '01') as fecha_ingreso,
      (select max(r.nombre_referente) from termino_referencia_af tdr, solicitud_capacitacion s, referente r
      where tdr.id_accion_formativa = af.id_accion_formativa and tdr.id_solicitud = s.id_solicitud and s.id_referente = r.id_referente) as referente,
      (select max(h.lastupdate) from informe_final_af_his h where h.id_accion_formativa = af.id_accion_formativa and h.id_estado_informe = 2) as fecha_pres_informe,
      (select max(h.lastupdate) from informe_final_af_his h where h.id_accion_formativa = af.id_accion_formativa and h.id_estado_informe in(4,5) ) as fecha_validacion,
      (select max(h.lastupdate) from informe_final_af_his h where h.id_accion_formativa = af.id_accion_formativa and h.id_estado_informe = 1) as fecha_finaliza_centro,
      (select max(h.lastupdate) from informe_final_af_his h where h.id_accion_formativa = af.id_accion_formativa and h.id_estado_informe = 3) as fecha_revision,
      (select max(h.lastupdate) from informe_final_af_his h where h.id_accion_formativa = af.id_accion_formativa and h.id_estado_informe = 17) as fecha_factura_recibida,
      (select max(h.lastupdate) from informe_final_af_his h where h.id_accion_formativa = af.id_accion_formativa and h.id_estado_informe = 11) as fecha_tramite_pago,
      (select v.contrato_compra from VACCION_FORMATIVA_CONTRATADA v where v.id_accion_formativa = af.id_accion_formativa) as contrato_compra,
      (select ac.num_item from accion_contratada ac, grupo_accion_contratada g where ac.id_accion_contratada = g.id_accion_contratada and g.id_accion_formativa = af.id_accion_formativa) as item,
      (select ac.codigo_programa from accion_contratada ac, grupo_accion_contratada g where ac.id_accion_contratada = g.id_accion_contratada and g.id_accion_formativa = af.id_accion_formativa) as codigo_programa,
      (select max(fecha_visita) from accion_formativa_segui s where s.id_accion_formativa = af.id_accion_formativa) as ult_fecha_visita,
      (select max(u.nombre) from accion_formativa_segui s, usuario u where s.usuario_creacion = u.usuario and s.id_accion_formativa = af.id_accion_formativa and s.fecha_visita = (select max(s.fecha_visita) from accion_formativa_segui s where s.id_accion_formativa = af.id_accion_formativa)) as ult_usuario_visita,
      (select mf.modalidad_formacion from modalidad_de_formacion mf where mf.id_modalidad_formacion = d.id_modalidad_formacion) as modalidad_formativa,
      (select ac.codigo_categ from accion_contratada ac, grupo_accion_contratada g where ac.id_accion_contratada = g.id_accion_contratada and g.id_accion_formativa = af.id_accion_formativa) as codigo_categ,
      (select ac.nombre_categ from accion_contratada ac, grupo_accion_contratada g where ac.id_accion_contratada = g.id_accion_contratada and g.id_accion_formativa = af.id_accion_formativa) as nombre_categ
      from accion_formativa_detalle d, accion_formativa af, estado_accion_formativa e
      where af.id_accion_formativa = d.id_accion_formativa 
          and af.codigo_estado_af = e.codigo_estado_af     
          and (d.id_programa_formacion = :id_programa_formacion or :id_programa_formacion = -1)
          and (d.no_convocatoria = :no_convocatoria or :no_convocatoria = -1)  
          and (af.id_proveedor_af = :id_proveedor_af or :id_proveedor_af = -1)    
          and (af.id_sitio_capacitacion = :id_sitio_capacitacion or :id_sitio_capacitacion = -1)   
          and (af.id_ejercicio = :ejercicio or :ejercicio is null)    
          and (e.codigo_estado_af = :codigo_estado_af or :codigo_estado_af is null) 
          and (e.codigo_estado_af &lt;&gt; '01')
          and (exists(select 1 from contrato_proveedor_af c where c.id_contrato = d.id_contrato and to_char(c.num_licitacion) = :num_licitacion) or :num_licitacion is null)
          and (exists(select 1 from contrato_proveedor_af c where c.id_contrato = d.id_contrato and c.id_ejercicio = :id_ejercicio_licitacion) or :id_ejercicio_licitacion is null) 
  )
  where clasificacion = :clasificacion or :clasificacion = -1
),

estado_informe as
(
  select cur.id_curso, max(e.nombre_estado_informe) as estado_informe
  from informe_final_af inf, estado_informe_af e, cursos cur
  where inf.id_accion_formativa = cur.id_curso and inf.id_estado_informe = e.id_estado_informe and e.orden = cur.orden          
  group by cur.id_curso
),

descrip_estado_informe as
(
  select cur.id_curso, max(e.descripcion_estado) as descripcion_estado
  from informe_final_af inf, estado_informe_af e, cursos cur
  where inf.id_accion_formativa = cur.id_curso and inf.id_estado_informe = e.id_estado_informe and e.orden = cur.orden  
  group by cur.id_curso
),

participantes as
(
  /*
  select cur.id_curso, count(1) as activos, decode(par.genero,'M','HOMBRES','MUJERES') as sexo
  from participante_af paf, cursos cur, participante par
  where paf.id_accion_formativa = cur.id_curso
  AND PAF.ID_PARTICIPANTE = PAR.ID_PARTICIPANTE
  and paf.es_capacitado = 1  
  group by cur.id_curso, par.genero
  */
  select cur.id_curso, 
         nvl( ( select count(paf.id_participante) 
                from participante_af paf, participante p 
                where paf.id_accion_formativa = cur.id_curso and paf.id_participante = p.id_participante and paf.es_capacitado = 1
                ) ,0) as activos, 
         nvl( ( select count(paf.id_participante) 
                from participante_af paf, participante p 
                where paf.id_accion_formativa = cur.id_curso and paf.id_participante = p.id_participante and paf.es_capacitado = 1 and p.genero = 'M'
                ) ,0) as hombres, 
         nvl( ( select count(paf.id_participante) 
                from participante_af paf, participante p 
                where paf.id_accion_formativa = cur.id_curso and paf.id_participante = p.id_participante and paf.es_capacitado = 1 and p.genero = 'F'
                ) ,0) as mujeres
  from cursos cur  
)

select t.*, case when activos = 0 then 0 else round(total_pago / activos,2) end as costo_x_participante
from(
    select c.id_programa_formacion, c.proveedor, c.sitio_capacitacion, c.id_curso, c.area_formativa, c.curso,
    c.fecha_inicio_real, c.fecha_fin_real, c.horario, c.nombre_estado_af, c.direccion, c.encargado, c.email, c.telefono,
    C.NOTAS, C.CODIGO_GRUPO, C.DURACION_HORAS, C.DEPARTAMENTO, C.MUNICIPIO, C.CLASIFICACION,
    C.NUM_CONTRATO, CASE 
                    WHEN (SELECT COUNT(1) FROM INFORME_FINAL_AF INF WHERE INF.ID_ACCION_FORMATIVA = C.ID_CURSO) &gt; 0 THEN    
                        (SELECT SUM(INF.MONTO_FACTURA) FROM INFORME_FINAL_AF INF WHERE INF.ID_ACCION_FORMATIVA = C.ID_CURSO)
                    WHEN (SELECT COUNT(1) FROM TERMINO_REFERENCIA_AF T WHERE T.ID_ACCION_FORMATIVA = C.ID_CURSO) &gt; 0 THEN                
                        (select sum(t.monto_adjudicado) from termino_referencia_af t where t.id_accion_formativa = c.id_curso)
                    else c.costo_x_participante * (select p.activos from participantes p where p.id_curso = c.id_curso)
                    end as total_pago, c.nombre_sub_area, c.adjudicado_subarea,
    (select p.hombres from participantes p where p.id_curso = c.id_curso) as hombres, 
    (select p.mujeres from participantes p where p.id_curso = c.id_curso) as mujeres, 
    (select p.activos from participantes p where p.id_curso = c.id_curso) as activos, 
    c.num_licitacion, c.ejercicio_licitacion, c.tecnico_asignado, c.actualizacion,
    (select e.estado_informe from estado_informe e where e.id_curso = c.id_curso) as estado_informe,
    (select e.descripcion_estado from descrip_estado_informe e  where e.id_curso = c.id_curso) as descripcion_estado_informe,
    c.fecha_ingreso, c.referente, c.fecha_pres_informe, c.fecha_validacion, c.fecha_finaliza_centro,
    c.fecha_revision, c.fecha_factura_recibida, c.fecha_tramite_pago,
    c.contrato_compra, c.item, c.codigo_programa, c.ult_fecha_visita, c.ult_usuario_visita, c.modalidad_formativa,
    c.codigo_categ, c.nombre_categ
    from cursos c
) t

