              <DbSource ConnectionRef="ConnectionString (MySettings)" DbObjectName="" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="FillFechaFacturaObservada" GenerateMethods="Both" GenerateShortCommands="true" GeneratorGetMethodName="GetFechaFacturaObservada" GeneratorSourceName="FillFechaFacturaObservada" GetMethodModifier="Public" GetMethodName="GetFechaFacturaObservada" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetFechaFacturaObservada" UserSourceName="FillFechaFacturaObservada">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select *
from (
    select 
      reci.lastupdate as fecha_recepcion,
      reci.usuario as usuario_recepcion,  
      (select p.nombre_programa_formacion from accion_formativa_detalle ad, programa_formacion p where ad.id_accion_formativa = af.id_accion_formativa and p.id_programa_formacion = ad.id_programa_formacion) as programa,
      af.nombre_accion_formativa as curso,
      (select min(h.lastupdate) from informe_final_af_his h where h.id_informe_final = inf.id_informe_final and h.id_estado_informe = 3) as fecha_en_revision,
      af.codigo_grupo,
      (select p.nombre_proveedor from proveedor_af p where p.id_proveedor_af = af.id_proveedor_af) as proveedor,  
      (select p.nit from proveedor_af p where p.id_proveedor_af = af.id_proveedor_af) as nit,
      (select max(f.numero_documento) from factura_af f, factura_det d where f.id_factura_af = d.id_factura_af and d.id_informe_final = inf.id_informe_final and f.estado &lt;&gt; 'A') as no_factura,
      inf.monto_factura,
      (select min(h.lastupdate) from informe_final_af_his h where h.id_informe_final = inf.id_informe_final and h.id_estado_informe in(21)) as fecha_devuelto,
      (select min(h.comentarios) from informe_final_af_his h where h.id_informe_final = inf.id_informe_final and h.id_estado_informe in(21)) as comentario_devuelto,
      (select min(h.lastupdate) from informe_final_af_his h where h.id_informe_final = inf.id_informe_final and h.id_estado_informe = 11) as fecha_factura_recibida,
      (select min(h.lastupdate) from informe_final_af_his h where h.id_informe_final = inf.id_informe_final and h.id_estado_informe = 21) as fecha_factura_observada,
      (select u.nombre from usuario u where u.usuario = inf.usuario_asignado) as usuario_asignado  
    from accion_formativa af, informe_final_af inf,
         (  select row_number() over(partition by h.id_informe_final order by h.id_informe_final, h.lastupdate) as num, h.id_informe_final, h.lastupdate, (select u.nombre from usuario u where u.usuario = h.userid) as usuario 
            from informe_final_af_his h 
            where h.id_estado_informe = 2 
          ) reci
    where af.id_accion_formativa = inf.id_accion_formativa
    and inf.id_informe_final = reci.id_informe_final
    and reci.num = 1
) s
WHERE to_date(to_char(fecha_factura_observada,'DD/MM/YYYY'),'DD/MM/YYYY') BETWEEN :FECHA_INI AND :FECHA_FIN</CommandText>
