              <DbSource ConnectionRef="ConnectionString (MySettings)" DbObjectName="" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="FillByParametros" GenerateMethods="Both" GenerateShortCommands="true" GeneratorGetMethodName="GetDataByParametros" GeneratorSourceName="FillByParametros" GetMethodModifier="Public" GetMethodName="GetDataByParametros" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetDataByParametros" UserSourceName="FillByParametros">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT *
FROM (
SELECT
  CASE
    WHEN (SELECT COUNT(1) FROM GRUPO_ACCION_CONTRATADA GC WHERE GC.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA) = 0 THEN 'LIBRE GESTION'
    ELSE (SELECT TC.NOM_TIPO_CONTRA FROM GRUPO_ACCION_CONTRATADA GC, ACCION_CONTRATADA AC, CONTRATO_BOLPROS C, TIPO_CONTRATACION TC 
          WHERE GC.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA
          AND GC.ID_ACCION_CONTRATADA = AC.ID_ACCION_CONTRATADA
          AND AC.ID_CONTRATO = C.ID_CONTRATO
          AND C.ID_TIPO_CONTRA = TC.ID_TIPO_CONTRA) 
  END AS TIPO_CONTRATACION,
  AD.ID_UNIDAD_ORGANIZATIVA, 
  ( SELECT CC.NO_CONTRATO_COMPRA FROM CONTRATO_COMPRA CC, GRUPO_ACCION_CONTRATADA GC, ACCION_CONTRATADA AC, CONTRATO_BOLPROS CB 
    WHERE GC.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA AND GC.ID_ACCION_CONTRATADA = AC.ID_ACCION_CONTRATADA AND AC.ID_CONTRATO = CB.ID_CONTRATO AND CC.ID_CONTRATO_COMPRA = CB.ID_CONTRATO_COMPRA) AS OFERTA_COMPRA, 
  CASE 
  WHEN (SELECT COUNT(1) FROM GRUPO_ACCION_CONTRATADA GC WHERE GC.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA) = 0 THEN TDR.RESOLUCION
  ELSE 
    ( SELECT CB.NUM_CONTRATO FROM GRUPO_ACCION_CONTRATADA GC, ACCION_CONTRATADA AC, CONTRATO_BOLPROS CB WHERE GC.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA AND GC.ID_ACCION_CONTRATADA = AC.ID_ACCION_CONTRATADA AND AC.ID_CONTRATO = CB.ID_CONTRATO)
  END AS CONTRATO,  
  CASE 
  WHEN (SELECT COUNT(1) FROM GRUPO_ACCION_CONTRATADA GC WHERE GC.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA) = 0 THEN TDR.TDR
  ELSE
    (SELECT TO_CHAR(AC.NUM_ITEM) FROM GRUPO_ACCION_CONTRATADA GC, ACCION_CONTRATADA AC  WHERE GC.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA AND GC.ID_ACCION_CONTRATADA = AC.ID_ACCION_CONTRATADA)  
  END AS NUMERO_ITEM,
  AF.DURACION_HORAS, 
  ROUND(TDR.MONTO_ADJUDICADO/ AF.DURACION_HORAS,4) AS COSTO_HORA_CURSO,
  TDR.MONTO_ADJUDICADO AS PRECIO_UNITARIO_CURSO,
  AF.NOTAS AS INSTRUCTOR, 
  (SELECT CR.CODIGO_CENTRO_RESPONSABILIDAD FROM GRUPO_SOLICITUD GS, SOLICITUD_CAPACITACION SC, CENTRO_RESPONSABILIDAD CR
  WHERE GS.TDR = TDR.TDR AND GS.CORRELATIVO_GRUPO = TDR.CORRELATIVO_GRUPO AND GS.ID_SOLICITUD = SC.ID_SOLICITUD 
  AND SC.ID_CENTRO_RESPONSABILIDAD = CR.ID_CENTRO_RESPONSABILIDAD) AS CENTRO_RESPONSABILIDAD,  
  (SELECT GS.CORRELATIVO_GRUPO FROM GRUPO_SOLICITUD GS
  WHERE GS.TDR = TDR.TDR AND GS.CORRELATIVO_GRUPO = TDR.CORRELATIVO_GRUPO) AS CODIGO_SIFP,
  (SELECT SC.ID_REFERENTE FROM GRUPO_SOLICITUD GS, SOLICITUD_CAPACITACION SC
  WHERE GS.TDR = TDR.TDR AND GS.CORRELATIVO_GRUPO = TDR.CORRELATIVO_GRUPO AND GS.ID_SOLICITUD = SC.ID_SOLICITUD) AS ID_REFERENTE,
  (SELECT R.NOMBRE_REFERENTE FROM GRUPO_SOLICITUD GS, SOLICITUD_CAPACITACION SC, REFERENTE R
  WHERE GS.TDR = TDR.TDR AND GS.CORRELATIVO_GRUPO = TDR.CORRELATIVO_GRUPO AND GS.ID_SOLICITUD = SC.ID_SOLICITUD AND SC.ID_REFERENTE = R.ID_REFERENTE) AS REFERENTE,
  P.NOMBRE_PROVEEDOR AS PROVEEDOR, 
   AF.NOMBRE_ACCION_FORMATIVA,
  (SELECT D.NOMBRE FROM DEPARTAMENTO D WHERE D.CODIGO_DEPARTAMENTO = AD.CODIGO_DEPARTAMENTO) AS DEPARTAMENTO, 
  (SELECT M.NOMBRE FROM MUNICIPIO M WHERE M.CODIGO_DEPARTAMENTO = AD.CODIGO_DEPARTAMENTO AND M.CODIGO_MUNICIPIO = AD.CODIGO_MUNICIPIO) AS MUNICIPIO,
  AD.LUGAR_EJECUCION, 
  AF.FECHA_INICIO_REAL, 
  AF.FECHA_FIN_REAL,
  EAF.NOMBRE_ESTADO_AF AS ESTADO_CURSO,  
  AF.HORARIO, AF.PARTICIPANTES_INSCRITOS, 
  NVL((SELECT COUNT(1) FROM PARTICIPANTE_AF PAF WHERE PAF.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA AND PAF.ES_CAPACITADO = 1),0) AS PARTICIPANTES_ACTIVOS, 
  NVL((SELECT COUNT(1) FROM PARTICIPANTE_AF PAF, SOLICITUD_INSCRIPCION_AF S WHERE PAF.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA AND PAF.ID_SOLICITUD = S.ID_SOLICITUD AND S.GENERO = 'M' AND PAF.ES_CAPACITADO = 1),0) AS HOMBRES_CAPACITADOS,
  NVL((SELECT COUNT(1) FROM PARTICIPANTE_AF PAF, SOLICITUD_INSCRIPCION_AF S WHERE PAF.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA AND PAF.ID_SOLICITUD = S.ID_SOLICITUD AND S.GENERO = 'F' AND PAF.ES_CAPACITADO = 1),0) AS MUJERES_CAPACITADAS,
  ROUND(NVL((SELECT COUNT(1) FROM PARTICIPANTE_AF PAF, SOLICITUD_INSCRIPCION_AF S WHERE PAF.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA AND PAF.ID_SOLICITUD = S.ID_SOLICITUD AND S.GENERO = 'M' AND PAF.ES_CAPACITADO = 1),0)*AF.COSTO_X_PARTICIPANTE,2) AS MONTO_HOMBRES_CAPACITADOS,
  ROUND(NVL((SELECT COUNT(1) FROM PARTICIPANTE_AF PAF, SOLICITUD_INSCRIPCION_AF S WHERE PAF.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA AND PAF.ID_SOLICITUD = S.ID_SOLICITUD AND S.GENERO = 'F' AND PAF.ES_CAPACITADO = 1),0)*AF.COSTO_X_PARTICIPANTE,2) AS MONTO_MUJERES_CAPACITADAS,
  AF.CODIGO_GRUPO, 
  (SELECT MAX(E.NOMBRE_ESTADO_INFORME) FROM INFORME_FINAL_AF INF, ESTADO_INFORME_AF E WHERE INF.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA AND INF.ID_ESTADO_INFORME = E.ID_ESTADO_INFORME) AS NOMBRE_ESTADO_INFORME,
  (SELECT MIN(H.LASTUPDATE) FROM ACCION_FORMATIVA_HIST H WHERE H.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA AND H.CODIGO_ESTADO_AF = '02') AS FECHA_INGRESO_SIS,
  (SELECT AREA.AREA_FORMACION FROM AREA_FORMACION AREA, CURSO_TEMA CT WHERE AREA.ID_AREA_FORMACION = CT.ID_AREA_FORMACION AND CT.ID_TEMA_CURSO = AF.ID_TEMA_CURSO) AS AREA_FORMACION,
  (SELECT SA.NOMBRE_SUBAREA FROM SUB_AREA_FORMACION SA, CURSO_TEMA CT WHERE CT.ID_TEMA_CURSO = AF.ID_TEMA_CURSO AND CT.ID_SUBAREA_FORMACION = SA.ID_SUBAREA_FORMACION) AS SUB_AREA,
  (SELECT MAX(E.NOMBRE_ESTADO_INFORME) FROM INFORME_FINAL_AF INF, ESTADO_INFORME_AF E WHERE INF.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA AND INF.ID_ESTADO_INFORME = E.ID_ESTADO_INFORME) AS ESTADO_INFORME,
  (SELECT GS.ID_SOLICITUD FROM GRUPO_ACCION_CONTRATADA GA, GRUPO_SOLICITUD GS WHERE GA.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA AND GA.ID_GRUPO_AF = GS.ID_GRUPO_SOLICITUD) AS ID_SOLICITUD,
  NVL((SELECT SUM(I.MONTO_FACTURA) FROM INFORME_FINAL_AF I WHERE I.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA ),0) AS MONTO_INFORME,
  ( SELECT CB.ES_INCREMENTO FROM GRUPO_ACCION_CONTRATADA GC, ACCION_CONTRATADA AC, CONTRATO_BOLPROS CB WHERE GC.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA AND GC.ID_ACCION_CONTRATADA = AC.ID_ACCION_CONTRATADA AND AC.ID_CONTRATO = CB.ID_CONTRATO) AS INCREMENTO,
  ( SELECT CB.NO_PRODUCTO FROM GRUPO_ACCION_CONTRATADA GC, ACCION_CONTRATADA AC, CONTRATO_BOLPROS CB WHERE GC.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA AND GC.ID_ACCION_CONTRATADA = AC.ID_ACCION_CONTRATADA AND AC.ID_CONTRATO = CB.ID_CONTRATO) AS NO_PRODUCTO,
  (select max(fecha_visita) from accion_formativa_segui s where s.id_accion_formativa = af.id_accion_formativa) as ult_fecha_visita,
  (select max(u.nombre) from accion_formativa_segui s, usuario u where s.usuario_creacion = u.usuario and s.id_accion_formativa = af.id_accion_formativa and s.fecha_visita = (select max(s.fecha_visita) from accion_formativa_segui s where s.id_accion_formativa = af.id_accion_formativa)) as ult_usuario_visita,
  (select mf.modalidad_formacion from modalidad_de_formacion mf where mf.id_modalidad_formacion = AD.id_modalidad_formacion) as modalidad_formativa,
  (select ac.codigo_categ from accion_contratada ac, grupo_accion_contratada g where ac.id_accion_contratada = g.id_accion_contratada and g.id_accion_formativa = af.id_accion_formativa) as codigo_categ,
  (select ac.nombre_categ from accion_contratada ac, grupo_accion_contratada g where ac.id_accion_contratada = g.id_accion_contratada and g.id_accion_formativa = af.id_accion_formativa) as nombre_categ
FROM ACCION_FORMATIVA AF, ACCION_FORMATIVA_DETALLE AD, PROVEEDOR_AF P, ESTADO_ACCION_FORMATIVA EAF,
  TERMINO_REFERENCIA_AF TDR, PROGRAMA_FORMACION PF
WHERE AF.ID_ACCION_FORMATIVA = AD.ID_ACCION_FORMATIVA
  AND TDR.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA
  AND AF.ID_PROVEEDOR_AF = P.ID_PROVEEDOR_AF  
  AND AF.CODIGO_ESTADO_AF = EAF.CODIGO_ESTADO_AF
  AND AD.ID_PROGRAMA_FORMACION  = PF.ID_PROGRAMA_FORMACION  
  AND PF.ID_PROGRAMA_FORMACION = :ID_PROGRAMA_FORMACION  
  AND (P.ID_PROVEEDOR_AF = :ID_PROVEEDOR OR :ID_PROVEEDOR = -1)    
)
WHERE (ID_UNIDAD_ORGANIZATIVA = :ID_UNIDAD_ORGANIZATIVA OR :ID_UNIDAD_ORGANIZATIVA = -1)
AND (CONTRATO = :CONTRATO OR :CONTRATO = ' ')
AND (NUMERO_ITEM = :ITEM OR :ITEM = ' ')
AND (ID_REFERENTE = :ID_REFERENTE OR :ID_REFERENTE = -1)
AND (OFERTA_COMPRA = :OFERTA_COMPRA OR :OFERTA_COMPRA = ' ')</CommandText>
