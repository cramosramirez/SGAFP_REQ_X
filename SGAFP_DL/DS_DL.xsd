AFD.LUGAR_EJECUCION,
INF.MONTO_PAGO_INICIAL, (INF.MONTO_PAGO_INICIAL - INF.MONTO_FACTURA) AS PORC_PENALIZACION
FROM INFORME_FINAL_AF INF, ACCION_FORMATIVA AF, 
ACCION_FORMATIVA_DETALLE AFD, PROVEEDOR_AF P, MODALIDAD_DE_FORMACION MO
WHERE INF.ID_ACCION_FORMATIVA = AF.ID_ACCION_FORMATIVA
AND AF.ID_ACCION_FORMATIVA = AFD.ID_ACCION_FORMATIVA
AND AF.ID_PROVEEDOR_AF = P.ID_PROVEEDOR_AF
AND AFD.ID_MODALIDAD_FORMACION = MO.ID_MODALIDAD_FORMACION
AND INF.ID_INFORME_FINAL = :ID_INFORME_FINAL</CommandText>
