              <DbSource ConnectionRef="ConnectionString (MySettings)" DbObjectName="" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="FillByAccionPeriodoReal" GenerateMethods="Both" GenerateShortCommands="true" GeneratorGetMethodName="GetDataByAccionPeriodoReal" GeneratorSourceName="FillByAccionPeriodoReal" GetMethodModifier="Public" GetMethodName="GetDataByAccionPeriodoReal" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetDataByAccionPeriodoReal" UserSourceName="FillByAccionPeriodoReal">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>WITH ACCION AS(
    SELECT AF.ID_ACCION_FORMATIVA, 
    P.NOMBRE_PROVEEDOR, 
    AF.NOMBRE_ACCION_FORMATIVA,       
    AF.CODIGO_GRUPO, 
    AF.HORARIO, 
    AF.FECHA_INICIO_REAL, 
    AF.FECHA_FIN_REAL,       
    PAR.NOMBRES, 
    PAR.APELLIDOS,    
    ASIS.FECHA, 
    NVL((SELECT DISTINCT 
             CASE
                WHEN S2.ASISTIO = 0 THEN 'NO'
                ELSE 'SI'
             END
             FROM ASISTENCIA_AF S1, ASISTENCIA_AF_DET S2 
             WHERE S1.ID_ASISTENCIA_AF = S2.ID_ASISTENCIA_AF 
                  AND S1.ID_ACCION_FORMATIVA = PAF.ID_ACCION_FORMATIVA 
                  AND S2.ID_PARTICIPANTE = PAR.ID_PARTICIPANTE 
                  AND S1.ID_ASISTENCIA_AF = ASIS.ID_ASISTENCIA_AF), 'SI') AS VALOR,
    NVL((SELECT DISTINCT S4.HORAS_ASISTENCIA FROM ASISTENCIA_AF S3, ASISTENCIA_AF_DET S4
             WHERE S3.ID_ASISTENCIA_AF = S4.ID_ASISTENCIA_AF
                  AND S3.ID_ACCION_FORMATIVA = PAF.ID_ACCION_FORMATIVA 
                  AND S4.ID_PARTICIPANTE = PAR.ID_PARTICIPANTE 
                  AND S3.ID_ASISTENCIA_AF = ASIS.ID_ASISTENCIA_AF), ASIS.CANTIDAD_HORAS) AS HORAS_ASISTIDAS,
    0 AS ID_PERIODO, '' AS PERIODO,  SYSDATE AS FECHA_INICIAL, SYSDATE AS FECHA_FINAL,  PAR.ID_PARTICIPANTE      
    FROM ACCION_FORMATIVA AF
    JOIN PARTICIPANTE_AF PAF ON AF.ID_ACCION_FORMATIVA = PAF.ID_ACCION_FORMATIVA 
    JOIN PROVEEDOR_AF P ON AF.ID_PROVEEDOR_AF = P.ID_PROVEEDOR_AF 
    JOIN ASISTENCIA_AF ASIS ON AF.ID_ACCION_FORMATIVA = ASIS.ID_ACCION_FORMATIVA
    JOIN PARTICIPANTE PAR ON PAF.ID_PARTICIPANTE = PAR.ID_PARTICIPANTE 
    WHERE AF.ID_ACCION_FORMATIVA = :ID_ACCION_FORMATIVA 
    AND PAF.ESTADO IN ('I','V','G')          
    AND ASIS.FECHA &gt;= :FECHA_INICIAL AND ASIS.FECHA &lt;= :FECHA_FINAL    
),

FICHA AS(
    SELECT FP.ID_PARTICIPANTE, FP.NUME_FICH, FP.ID_FUENTE, FP.NO_CONVOCATORIA
    FROM FICHA_PATI FP
    WHERE EXISTS( SELECT 1 FROM ACCION_FORMATIVA_DETALLE D
                  WHERE D.ID_ACCION_FORMATIVA = :ID_ACCION_FORMATIVA AND
                  D.ID_FUENTE = FP.ID_FUENTE AND 
                  D.NO_CONVOCATORIA = FP.NO_CONVOCATORIA AND
                  D.CODIGO_DEPARTAMENTO = FP.DEPARTAMENTO_RESIDENCIA AND
                  D.CODIGO_MUNICIPIO = FP.MUNICIPIO_RESIDENCIA
                )
),

CONVENIO AS(
    SELECT DF.NUME_FAMI, DF.NUME_FICH_PATI, DF.ID_FUENTE, DF.NO_CONVOCATORIA, 
    CASE 
     WHEN DF.TIPO_DOCU_IDEN = 1 AND DF.NUME_DOCU_IDEN IS NOT NULL THEN DF.NUME_DOCU_IDEN
     ELSE (SELECT RTRIM(NUME_CARN_MINO) FROM ENCA_FICH_PATI WHERE ENCA_FICH_PATI.NUME_FICH = DF.NUME_FICH AND LENGTH(RTRIM(NUME_CARN_MINO))&gt;1 )
    END AS DUI_CARNET_MIN
    FROM DETA_FAMI DF
    WHERE EXISTS( SELECT 1 FROM ACCION_FORMATIVA_DETALLE D
                  WHERE D.ID_ACCION_FORMATIVA = :ID_ACCION_FORMATIVA AND
                  D.ID_FUENTE = DF.ID_FUENTE AND 
                  D.NO_CONVOCATORIA = DF.NO_CONVOCATORIA
                ) AND
    EXISTS(SELECT 1 FROM FICHA F WHERE F.NUME_FICH = DF.NUME_FICH_PATI) AND
    DF.ESTA = 'A'
    
)

SELECT AF.ID_ACCION_FORMATIVA, AF.NOMBRE_PROVEEDOR, AF.NOMBRE_ACCION_FORMATIVA, AF.CODIGO_GRUPO, 
AF.HORARIO, AF.FECHA_INICIO_REAL, AF.FECHA_FIN_REAL, AF.NOMBRES, AF.APELLIDOS, 
C.NUME_FAMI AS NUM_CONVENIO, FP.NUME_FICH AS NUM_FICHA_PATI, C.DUI_CARNET_MIN,
AF.FECHA, AF.VALOR, AF.HORAS_ASISTIDAS, AF.ID_PERIODO, AF.PERIODO, AF.FECHA_INICIAL, AF.FECHA_FINAL, AF.ID_PARTICIPANTE  
FROM ACCION AF, FICHA FP, CONVENIO C
WHERE AF.ID_PARTICIPANTE = FP.ID_PARTICIPANTE AND
FP.ID_FUENTE = C.ID_FUENTE AND 
FP.NO_CONVOCATORIA = C.NO_CONVOCATORIA AND
FP.NUME_FICH = C.NUME_FICH_PATI
ORDER BY AF.FECHA, AF.APELLIDOS, AF.NOMBRES</CommandText>
