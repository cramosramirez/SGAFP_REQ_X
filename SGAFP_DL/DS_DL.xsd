          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="ResumenEjecucion_MontosCapacitadosTableAdapter" GeneratorDataComponentClassName="ResumenEjecucion_MontosCapacitadosTableAdapter" Name="ResumenEjecucion_MontosCapacitados" UserDataComponentName="ResumenEjecucion_MontosCapacitadosTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="ConnectionString (MySettings)" DbObjectName="" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>with 
proveedores as
( 
  select p.id_proveedor_af, p.nombre_proveedor as proveedor,
  c.monto_asignado as adjudicado
  from accion_formativa af, proveedor_af p, contrato_provee_af c
  where af.id_proveedor_af = p.id_proveedor_af
  and af.id_proveedor_af = c.id_proveedor_af and af.id_ejercicio = c.id_ejercicio  
  and af.id_ejercicio = :id_ejercicio  
  and not exists(select 1 from termino_referencia_af tdr where tdr.id_accion_formativa = af.id_accion_formativa)
  group by p.id_proveedor_af, p.nombre_proveedor, c.monto_asignado
),
ejecutado as
(
  select id_proveedor_af, min(fecha_inicio_real) as inicio, max(fecha_fin_real) as fin, 
  sum(costo_x_participante) as monto, count(id_solicitud) as participantes
  from vsolic_ingresadas_htp 
  where ejercicio = :id_ejercicio and fecha_fin_real &lt; to_char(sysdate,'dd/MON/yyyy')
  group by id_proveedor_af
),
ejecucion as
(
  select id_proveedor_af, min(fecha_inicio_real) as inicio, max(fecha_fin_real) as fin, 
  sum(costo_x_participante) as monto, count(id_solicitud) as participantes
  from vsolic_ingresadas_htp 
  where ejercicio = :id_ejercicio and fecha_inicio_real &lt;= to_char(sysdate,'dd/MON/yyyy') and fecha_fin_real &gt;= to_char(sysdate,'dd/MON/yyyy')
  group by id_proveedor_af
),
programado_no_inscritos as
(
  select af.id_proveedor_af as id_proveedor_af, min(af.fecha_inicio_real) as inicio, max(af.fecha_fin_real) as fin,
  sum(af.costo_x_participante * af.numero_participantes) monto, sum(af.numero_participantes) as participantes
  from accion_formativa af
  where af.id_ejercicio = :id_ejercicio and af.fecha_inicio_real &gt; to_char(sysdate,'dd/MON/yyyy') 
  and af.participantes_inscritos = 0
  and not exists(select 1 from termino_referencia_af tdr where tdr.id_accion_formativa = af.id_accion_formativa)
  group by af.id_proveedor_af
),
programado as
(
  select id_proveedor_af, min(inicio) as inicio, max(fin) as fin, sum(monto) as monto, sum(participantes) as participantes
  from
  (
    select id_proveedor_af, min(fecha_inicio_real) as inicio, max(fecha_fin_real) as fin, 
    sum(costo_x_participante) as monto, count(id_solicitud) as participantes
    from vsolic_ingresadas_htp 
    where ejercicio = :id_ejercicio and fecha_inicio_real &gt; to_char(sysdate,'dd/MON/yyyy') 
    group by id_proveedor_af  
    
    union all
    
    select id_proveedor_af, inicio, fin, monto, participantes
    from programado_no_inscritos 
  )
  group by id_proveedor_af 
),
participantes as(
  select sum(total) as total
  from 
  (
  select nvl(count(id_solicitud),0) as total
  from vsolic_ingresadas_htp 
  where ejercicio = :id_ejercicio
  
  union all
  
  select sum(af.numero_participantes) as participantes
  from accion_formativa af
  where af.id_ejercicio = :id_ejercicio and af.fecha_inicio_real &gt; to_char(sysdate,'dd/MON/yyyy') 
  and af.participantes_inscritos = 0
  and not exists(select 1 from termino_referencia_af tdr where tdr.id_accion_formativa = af.id_accion_formativa)
  )
),

total_adjudicado as(
  select sum(monto_asignado) as total from contrato_provee_af where id_ejercicio = :id_ejercicio
)


select proveedores.proveedor,
      proveedores.adjudicado,
      ejecutado.inicio as ejecutado_inicio,
      ejecutado.fin as ejecutado_fin,
      nvl(ejecutado.monto,0) as ejecutado_monto,
      round(nvl(ejecutado.monto/(select total from total_adjudicado),0)*100,3) as ejecutado_porc,
      nvl(ejecutado.participantes,0) as ejecutado_participantes,      
      round(nvl(ejecutado.participantes,0)/(select total from participantes)*100,3) as ejecutado_part_porc,
      ejecucion.inicio as ejecucion_inicio,
      ejecucion.fin as ejecucion_fin,
      nvl(ejecucion.monto,0) as ejecucion_monto,
      round(nvl(ejecucion.monto/(select total from total_adjudicado),0)*100,3) as ejecucion_porc,
      nvl(ejecucion.participantes,0) as ejecucion_participantes,
      round(nvl(ejecucion.participantes,0)/(select total from participantes)*100,3) as ejecucion_part_porc,
      programado.inicio as programado_inicio,
      programado.fin as programado_fin,
      nvl(programado.monto,0) as programado_monto,
      round(nvl(programado.monto/(select total from total_adjudicado),0)*100,3) as programado_porc,
      nvl(programado.participantes,0) as programado_participantes,     
      round(nvl(programado.participantes,0)/(select total from participantes)*100,3) as programado_part_porc
from proveedores 
  left outer join ejecutado on proveedores.id_proveedor_af = ejecutado.id_proveedor_af 
  left outer join ejecucion on proveedores.id_proveedor_af = ejecucion.id_proveedor_af
  left outer join programado on proveedores.id_proveedor_af = programado.id_proveedor_af  
order by (nvl(ejecutado.monto,0) + nvl(ejecucion.monto,0) + nvl(programado.monto,0)) desc</CommandText>
