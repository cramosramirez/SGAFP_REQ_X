                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT *
FROM (
  SELECT 
      CB.ID_CONTRATO_COMPRA,
      (SELECT EXTRACT(YEAR FROM M.FECHA_INGRESO) FROM CONTRATO_COMPRA M WHERE M.ID_CONTRATO_COMPRA = CB.ID_CONTRATO_COMPRA) AS ANNIO,
      (SELECT M.NO_CONTRATO_COMPRA FROM CONTRATO_COMPRA M WHERE M.ID_CONTRATO_COMPRA = CB.ID_CONTRATO_COMPRA) AS NO_CONTRATO_COMPRA,
      GS.ID_SOLICITUD, 
      (SELECT SC.FECHA_SOLICITUD FROM SOLICITUD_CAPACITACION SC WHERE SC.ID_SOLICITUD = GS.ID_SOLICITUD) AS FECHA_SOLICITUD,
      AC.CANT_CURSOS AS CANT_CURSOS_X_ITEM,
      CB.NUM_CONTRATO, AC.NUM_ITEM, CB.MONTO AS MONTO_CONTRATO,
      CB.ID_PROVEEDOR_AF,
      (SELECT P.NOMBRE_PROVEEDOR FROM PROVEEDOR_AF P WHERE P.ID_PROVEEDOR_AF = CB.ID_PROVEEDOR_AF) AS NOMBRE_PROVEEDOR,
      NVL(GS.NOMBRE_ACCION_FORMATIVA, AC.NOMBRE_ACCION_FORMATIVA) AS NOMBRE_ACCION_FORMATIVA, 
      (SELECT CT.ID_AREA_FORMACION FROM CURSO_TEMA CT WHERE CT.CODIGO_PROGRAMA = GS.CODIGO_PROGRAMA) AS ID_AREA_FORMACION,
      (SELECT A.AREA_FORMACION FROM CURSO_TEMA CT, AREA_FORMACION A WHERE CT.ID_AREA_FORMACION = A.ID_AREA_FORMACION AND CT.CODIGO_PROGRAMA = GS.CODIGO_PROGRAMA) AS AREA_FORMATIVA,
      NVL(GS.DURACION_HORAS,AC.DURACION) AS DURACION_HORAS,  ROUND(AC.MONTO / DECODE(AC.CANT_CURSOS,0,NULL),2) AS MONTO_X_CURSO,
      AC.CODIGO_DEPARTAMENTO,
      (SELECT D.NOMBRE FROM DEPARTAMENTO D WHERE D.CODIGO_DEPARTAMENTO = AC.CODIGO_DEPARTAMENTO) AS DEPARTAMENTO,
      AC.CODIGO_MUNICIPIO,      
      (SELECT M.NOMBRE FROM MUNICIPIO M WHERE M.CODIGO_DEPARTAMENTO = AC.CODIGO_DEPARTAMENTO AND M.CODIGO_MUNICIPIO = AC.CODIGO_MUNICIPIO) AS MUNICIPIO,
      GS.LUGAR_EJECUCION, GS.NUM_PARTICIPANTES AS CANT_PARTI_SOLICITUD, 
      (SELECT R.NOMBRE_REFERENTE FROM REFERENTE R, SOLICITUD_CAPACITACION SC WHERE R.ID_REFERENTE = SC.ID_REFERENTE AND SC.ID_SOLICITUD = GS.ID_SOLICITUD) AS NOMBRE_REFERENTE,
