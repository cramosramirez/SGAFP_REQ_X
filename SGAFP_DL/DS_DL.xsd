                      <Parameter AllowDbNull="true" AutogeneratedName="FECHA_FINAL" ColumnName="FECHA" DataSourceName="" DataTypeServer="DATE(7)" DbType="DateTime" Direction="Input" ParameterName="FECHA_FINAL" Precision="0" ProviderType="DateTime" Scale="0" Size="7" SourceColumn="FECHA" SourceColumnNullMapping="false" SourceVersion="Current" />
