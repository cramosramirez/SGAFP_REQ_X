                    <CommandText>WITH INFORMES AS(
    SELECT  P.NOMBRE_PROVEEDOR, AF.NOMBRE_ACCION_FORMATIVA, AF.CODIGO_GRUPO, 
    AF.FECHA_INICIO_REAL, AF.FECHA_FIN_REAL, CONCAT(CONCAT(S.APELLIDOS, ', '), 
    S.NOMBRES) AS NOMBRES_APELLIDOS, INF2.NOTA_TEORICA, INF2.NOTA_PRACTICA, 
    INF2.NOTA_OBTENIDA AS NOTA_FINAL, INF2.PORC_ASISTENCIA, INF2.NOTAS, 
    AF.HORARIO, AF.ID_EJERCICIO, INF2.ID_PARTICIPANTE, 
    (SELECT DT.NOMBRE FROM DEPARTAMENTO DT WHERE DT.CODIGO_DEPARTAMENTO = D.CODIGO_DEPARTAMENTO) AS NOMBRE_DEPARTAMENTO, 
    (SELECT M.NOMBRE FROM MUNICIPIO M WHERE M.CODIGO_DEPARTAMENTO = D.CODIGO_DEPARTAMENTO AND M.CODIGO_MUNICIPIO = D.CODIGO_MUNICIPIO) AS NOMBRE_MUNICIPIO,
    NVL('','') AS NUM_CONVENIO, '' AS NUM_FICHA_PATI,                    
    '' AS DUI_CARNET_MIN, PROYS_ACCION_FORMATIVA_PATI(AF.ID_ACCION_FORMATIVA) AS CODIGO_PROYECTOS,    
    D.ID_FUENTE, D.NO_CONVOCATORIA
    FROM ACCION_FORMATIVA AF
    JOIN INFORME_FINAL_AF INF1 ON AF.ID_ACCION_FORMATIVA = INF1.ID_ACCION_FORMATIVA
    JOIN INFORME_FINAL_DET INF2 ON INF1.ID_INFORME_FINAL = INF2.ID_INFORME_FINAL 
    JOIN PROVEEDOR_AF P ON AF.ID_PROVEEDOR_AF = P.ID_PROVEEDOR_AF
    JOIN SOLICITUD_INSCRIPCION_AF S ON AF.ID_ACCION_FORMATIVA = S.ID_ACCION_FORMATIVA AND INF2.ID_PARTICIPANTE = S.ID_PARTICIPANTE
    JOIN ACCION_FORMATIVA_DETALLE D ON AF.ID_ACCION_FORMATIVA = D.ID_ACCION_FORMATIVA
